CREATE TABLE users (    id SERIAL PRIMARY KEY,    email VARCHAR(255) UNIQUE NOT NULL,    fam VARCHAR(255) NOT NULL,    name VARCHAR(255) NOT NULL,    otc VARCHAR(255),    phone VARCHAR(20) NOT NULL);CREATE TABLE coords (    id SERIAL PRIMARY KEY,    latitude FLOAT NOT NULL,    longitude FLOAT NOT NULL,    height INTEGER NOT NULL);CREATE TABLE pereval_added (    id SERIAL PRIMARY KEY,    user_id INTEGER REFERENCES users(id) NOT NULL,    coords_id INTEGER REFERENCES coords(id) NOT NULL,    beauty_title VARCHAR(255) NOT NULL,    title VARCHAR(255) NOT NULL,    other_titles VARCHAR(255),    connect VARCHAR(255),    add_time TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,    status VARCHAR(20) NOT NULL DEFAULT 'new',    winter_level VARCHAR(20),    summer_level VARCHAR(20),    autumn_level VARCHAR(20),    spring_level VARCHAR(20),    CHECK (status IN ('new', 'pending', 'accepted', 'rejected')));CREATE TABLE pereval_images (    id SERIAL PRIMARY KEY,    pereval_id INTEGER REFERENCES pereval_added(id) NOT NULL,    image TEXT NOT NULL,    title VARCHAR(255) NOT NULL);